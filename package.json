{
  "name": "stand",
  "version": "1.0.0",
  "description": "My first project applying all the knowledge acquired in web design",
  "main": "server.js",
  "config": {
    "src_dir": "src",
    "public_dir": "public",
    "dist_dir": "dist",
    "environment_dir": "environment"
  },
  "scripts": {
    "start": "npm run build && npm run server",
    "dev": "cross-env NODE_ENV=development node automated_tasks/tasks/devenv.config.js",
    "server": "node server.js",
    "build": "cross-env NODE_ENV=production webpack -p",
    "distribute": "cross-env NODE_ENV=distribution webpack -p",
    "watch": "webpack --watch",
    "test": "jest",
    "preversion": "npm test",
    "version": "npm run distribute && git add -A dist",
    "postversion": "git push && git push --tags && rm -rf build/temp"
  },
  "jest": {
    "testPathDirs": [
      "src/test"
    ],
    "testRegex": ".*.test.js",
    "testEnvironment": "node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IrinaMarques/stand.git"
  },
  "directories": {
    "doc": "./doc",
    "environment": "./environment",
    "dist": "./dist",
    "src": "./src",
    "public": "./public"
  },
  "author": "irinavpmarques@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/IrinaMarques/stand/issues"
  },
  "homepage": "https://github.com/IrinaMarques/stand#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-jest": "^18.0.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^2.0.0-alpha.12",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-register": "^6.18.0",
    "bluebird": "^3.5.0",
    "browser-sync": "^2.18.12",
    "clean-webpack-plugin": "^0.1.14",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "es6-promise": "^4.1.1",
    "es6-promise-promise": "^1.0.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.9.0",
    "generate-asset-webpack-plugin": "^0.3.0",
    "html-webpack-plugin": "^2.29.0",
    "jest": "^18.1.0",
    "live-server": "^1.1.0",
    "netutil": "0.0.2",
    "node-notifier": "^4.6.1",
    "node-sass": "^4.5.3",
    "object-assign": "^4.1.0",
    "sass-loader": "^6.0.5",
    "sinon": "^1.17.6",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.9",
    "webpack": "^3.3.0"
  },
  "dependencies": {
    "bundle-loader": "^0.5.5",
    "font-awesome-sass": "^4.7.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router-dom": "^4.1.2"
  }
}
